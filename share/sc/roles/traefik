role_traefik_services="traefik"

valueOf_traefik_network() {
    echo inherit
}
role_traefik_install() {
    local _s

    for _s in $role_traefik_services; do
	svc_install "$_s"
    done
}

role_traefik_config() {
    local _s

    portdb_init
    for _s in $role_traefik_services; do
	svc_config "$_s"
    done
}

role_traefik_start() {
    role_traefik_config

    service traefik start
}
# Local Variables:
# mode: sh
# End:
